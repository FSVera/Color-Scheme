set nu
source ~/.vim/vundle.vim

autocmd FileType php set omnifunc=phpcomplete#CompletePHP

"""""""""""""""""""""""""""""""""""""""""
"	    PALETA DE COLORES               "

source ~/.vim/colors/solarized.vim

	syntax enable
	set background=light
	let g:solarized_termcolors=256
	colorscheme solarized

"										"
